{
  "name": "bigin-class-hints",
  "displayName": "Bigin Common Class hints",
  "description": "Intelligent class name suggestions extracted from your LESS files in real-time",
  "version": "0.3.3",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension.js",
  "publisher": "Bigin-UI-Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/hithesh-m-20851/Bigin-UI-Team/tree/main/bigin-class-hints"
  },
  "contributes": {},
  "scripts": {
    "compile": "esbuild src/extension.ts --bundle --platform=node --outfile=dist/extension.js --external:vscode",
    "watch": "esbuild src/extension.ts --bundle --platform=node --outfile=dist/extension.js --external:vscode --watch"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.70.0",
    "esbuild": "^0.19.0",
    "typescript": "^5.0.0"
  }
}
